      case "SHORT":
        return SHORT;
      case "LONG":
        return LONG;
      default:
        return new PostType(Value.UNKNOWN, upperCasedValue);
    }
  }

  public enum Value {
    LONG,

    SHORT,

    UNKNOWN
  }

