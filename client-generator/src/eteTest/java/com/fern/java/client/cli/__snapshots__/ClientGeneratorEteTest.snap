package com.fern.basic;

import com.fern.basic.core.BearerAuth;
import com.fern.basic.core.Environment;
import com.fern.basic.resources.raven.device.DeviceServiceClient;
import java.util.Objects;
import java.util.concurrent.atomic.AtomicReference;
import java.util.function.Supplier;

public final class DeviceClient {
  private final Supplier<DeviceServiceClient> deviceServiceClient;

  public DeviceClient(String apiVersion) {
    this(Environment.PROD, apiVersion);
  }

  public DeviceClient(BearerAuth auth, String apiVersion) {
    this(Environment.PROD, auth, apiVersion);
  }

  public DeviceClient(Environment environment, String apiVersion) {
    this.deviceServiceClient =
        memoize(() -> new DeviceServiceClient(environment.getUrl(), apiVersion));
  }

  public DeviceClient(Environment environment, BearerAuth auth, String apiVersion) {
    this.deviceServiceClient =
        memoize(() -> new DeviceServiceClient(environment.getUrl(), auth, apiVersion));
  }

  public final DeviceServiceClient device() {
    return this.deviceServiceClient.get();
  }

  private static <T> Supplier<T> memoize(Supplier<T> delegate) {
    AtomicReference<T> value = new AtomicReference<>();
    return () -> {
      T val = value.get();
      if (val == null) {
        val = value.updateAndGet(cur -> cur == null ? Objects.requireNonNull(delegate.get()) : cur);
      }
      return val;
    };
  }
}

]


