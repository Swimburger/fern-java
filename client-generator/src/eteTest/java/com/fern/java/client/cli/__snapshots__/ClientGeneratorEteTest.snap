package com.fern.basic.resources.raven.user;

import com.fern.basic.core.BearerAuth;
import com.fern.basic.resources.raven.user.endpoints.CreateOrUpdate;
import com.fern.basic.resources.raven.user.endpoints.Get;
import com.fern.basic.resources.raven.user.exceptions.CreateOrUpdateException;
import com.fern.basic.resources.raven.user.exceptions.GetException;
import com.fern.basic.resources.raven.user.types.RavenUser;
import java.util.Optional;

public final class UserServiceClient {
  private final UserService service;

  private final String apiVersion;

  private final Optional<BearerAuth> auth;

  public UserServiceClient(String url, String apiVersion) {
    this.service = UserService.getClient(url);
    this.apiVersion = apiVersion;
    this.auth = Optional.empty();
  }

  public UserServiceClient(String url, BearerAuth auth, String apiVersion) {
    this.service = UserService.getClient(url);
    this.auth = Optional.of(auth);
    this.apiVersion = apiVersion;
  }

  /**
   * Creates an AppUser if doesn't exist already, or updates user properties
   *
   * @param request Wrapper object for the request body that includes any path parameters, query
   *     parameters, and headers
   * @throws CreateOrUpdateException Exception that wraps all possible endpoint errors
   * @return
   *     <p>updated or newly created user.
   */
  public RavenUser createOrUpdate(CreateOrUpdate.Request request) throws CreateOrUpdateException {
    BearerAuth authValue =
        request
            .getAuthOverride()
            .orElseGet(() -> this.auth.orElseThrow(() -> new RuntimeException("Auth is required")));
    return this.service.createOrUpdate(
        authValue, apiVersion, request.getAppId(), request.getBody());
  }

  /**
   * Gets the requested user
   *
   * @param request Wrapper object that includes any path parameters, query parameters, and headers
   * @throws GetException Exception that wraps all possible endpoint errors
   * @return RavenUser
   */
  public RavenUser get(Get.Request request) throws GetException {
    BearerAuth authValue =
        request
            .getAuthOverride()
            .orElseGet(() -> this.auth.orElseThrow(() -> new RuntimeException("Auth is required")));
    return this.service.get(authValue, apiVersion, request.getAppId(), request.getUserId());
  }
}

